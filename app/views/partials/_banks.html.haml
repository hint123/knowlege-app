- unless banks.blank? 
  %table{border: "2",:class=>"table table-condensed info"}
    %tr{:style=>"cellspacing='5'"}
      %th Board name
      %th Post name
      %th Qualification
      %th Last date
      %th More
      %th Type
    - banks.each do |job|
      %tr
        %td= job.post_name
        %td= job.qualification
        %td= job.last_date
        %td= link_to job.more,'#'

    
